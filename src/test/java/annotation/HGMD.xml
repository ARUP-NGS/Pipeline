<Pipeline>

<InputVCF class="buffer.VCFFile" filename="src/test/java/testvcfs/testAnnotation.vcf" />

<VariantPool class="buffer.variant.VariantPool">
     <InputVCF />
</VariantPool>

<VarsToGenes class="operator.gene.GeneListFromVariants">
 <VariantPool />
 <Genes class="buffer.GeneList" />
</VarsToGenes>

<HGMDVar class="operator.variant.HGMDVarAnnotator">
  <VariantPool />
</HGMDVar>

<HGMDGene class="operator.gene.HGMDAnnotator">
 <Genes />
</HGMDGene>

<ViewerFile class="operator.writer.VarViewerWriter">
        <VariantPool />
        <Genes />
        <ViewerCSV class="buffer.CSVFile" filename="HGDM_TESTING.csv" />
</ViewerFile>

</Pipeline>