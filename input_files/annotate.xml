
<!-- Uses annovar to annotate the given VCF file. This will produce lots of different output files... -->
<Pipeline>


 <annotate class="operator.annovar.AnnovarAnnotate">
   <input>
     <annovarInput class="buffer.VCFFile" filename="your_input_variants.vcf" />
   </input>
   <output>
     <annotations class="buffer.CSVFile" filename="annovar.output.csv" />
   </output>
 </annotate>


</Pipeline>
