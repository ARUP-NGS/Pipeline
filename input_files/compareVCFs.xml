
<!-- Compare two vcf files to determine amount of overlap, quality scores, etc. 
     This also produces two files containing all sites unique to A and unique to B
-->

<Pipeline>
 
 <Compare class="operator.CompareVCF">
   <input>
    <testA class="buffer.VCFFile" filename="your_variantsA.vcf" />
    <testB class="buffer.VCFFile" filename="your_variantsB.vcf" />
   </input>
   <output>
     <report class="buffer.TextBuffer" filename="comparison_report.txt" />
   </output>
 </Compare>
 

</Pipeline>
